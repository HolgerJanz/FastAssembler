;===================================
;FA Test Suite BW-DOS
;Requires:
;  >DOS>FA.COM,TESTS,
;  <EXAMPLES,<SOURCES 
;===================================
;Begin Test FA for BW-DOS
;===================================
; TEST: Statements
FA     /EQ   TEST6502
RUN A: /EQ   TESTBLCK
RUN A: /EQ   TESTCOMP
RUN A: /EQ   TESTEND
RUN A: /EQ   TESTEXPR
RUN A: /SELQ TESTINC2
RUN A: /SEQ  TESTLABE
RUN A: /SEQ  TESTMAXP
RUN A: /EQ   TESTMCLR
RUN A: /EQ   TESTPSDO
RUN A: /EQ   TESTSYMB
RUN A: /EQ   TESTINS
RUN A: /EQ   TESTORG1
RUN A: /EQ   TESTORG2
RUN A: /EQ   TESTBLK1
RUN A: /EQ   TESTPADD
COMP   TEST6502.COM TEST6502.OBJ 3
RUN A: TESTBLCK.COM TESTBLCK.OBJ 3
RUN A: TESTCOMP.COM TESTCOMP.OBJ 3
RUN A: TESTEND.COM  TESTEND.OBJ  3
RUN A: TESTEXPR.COM TESTEXPR.OBJ 3
RUN A: TESTINC2.COM TESTINC2.OBJ 3
RUN A: TESTLABE.COM TESTLABE.OBJ 3
RUN A: TESTMAXP.COM TESTMAXP.OBJ 3
RUN A: TESTMCLR.COM TESTMCLR.OBJ 3
RUN A: TESTPSDO.COM TESTPSDO.OBJ 3
RUN A: TESTSYMB.COM TESTSYMB.OBJ 3
RUN A: TESTINS.COM TESTINS.OBJ   3
RUN A: TESTORG1.COM TESTORG1.OBJ 3
RUN A: TESTORG2.COM TESTORG2.OBJ 3
RUN A: TESTBLK1.COM TESTBLK1.OBJ 3
RUN A: TESTPADD.COM TESTPADD.OBJ 3
;===================================
; TEST: error messages
PRINT TESTEMSG.TRC
FA TESTEMSG /Q
FA ERRORLNG /Q
FA ERRORURS /Q
FA ERRORN2  /Q
FA ERRORREC /Q
FA ERROR225 /Q
FA ERROR226 /Q
FA ERROR227 /Q
FA ERROR228 /Q
FA ERROR229 /Q
FA ERROR231 /Q
FA ERROR233 /Q
FA ERROR238 /Q
FA ERROR243 /Q
FA ERROR245 /Q
FA ERROR246 /Q
RUN A: ERRORPAD /Q
PRINT
COMP TESTEMSB.TXT TESTEMSG.TRC 3
DEL *.OBJ
DEL *.TRC
;===================================
; TEST: SDX Lib
CD <SDXLIB
FA /EQ TESTCOPY
RUN A: /EQ TESTFILE
RUN A: /EQ TESTGETN
RUN A: /EQ TESTMUDI
RUN A: /EQ TESTPRNF
RUN A: /EQ TESTSLSH
COMP TESTCOPY.COM TESTCOPY.OBJ 3
RUN A: TESTFILE.COM TESTFILE.OBJ 3
RUN A: TESTGETN.COM TESTGETN.OBJ 3
RUN A: TESTMUDI.COM TESTMUDI.OBJ 3
RUN A: TESTPRNF.COM TESTPRNF.OBJ 3
RUN A: TESTSLSH.COM TESTSLSH.OBJ 3
DEL *.OBJ
;===================================
; TEST: Examples
CD <EXAMPLES
FA /EQ ANTIC
RUN A: /EQ CALLEE
RUN A: /EQ CALLER
RUN A: /EQ COMP_BWD
RUN A: /EQ COMP_SDX
RUN A: /EQ GTIA
RUN A: /ELYQ HELLO
RUN A: /EQ HI_BWD
RUN A: /EQ HI_SDX
RUN A: /EQ HSCROL
RUN A: /EQ MY_COMP
RUN A: /EQ POKEY
RUN A: /EYQ RAINBOW
RUN A: /EQ TSR
RUN A: /EQ TSR_CALL
COMP ANTIC.COM ANTIC.OBJ 3
RUN A: CALLEE.COM CALLEE.OBJ 3
RUN A: CALLER.COM CALLER.OBJ 3
RUN A: COMP_BWD.COM COMP_BWD.OBJ 3
RUN A: COMP_SDX.COM COMP_SDX.OBJ 3
RUN A: GTIA.COM GTIA.OBJ 3
RUN A: HELLO.COM HELLO.OBJ 3
RUN A: HI_BWD.COM HI_BWD.OBJ 3
RUN A: HI_SDX.COM HI_SDX.OBJ 3
RUN A: HSCROL.COM HSCROL.OBJ 3
RUN A: MY_COMP.COM MY_COMP.OBJ 3
RUN A: POKEY.COM POKEY.OBJ 3
RUN A: RAINBOW.COM RAINBOW.OBJ 3
RUN A: <TESTS>RAINBOWB.TXT RAINBOW.SYM 3
RUN A: TSR.COM TSR.OBJ 3
RUN A: TSR_CALL.COM TSR_CALL.OBJ 3
DEL *.OBJ
DEL *.SYM
FA /EQ CART8K
COMP CART8K.ROM CART8K.OBJ
DEL *.OBJ
;===================================
; TEST: FA compiles itself
CD <SOURCES
FA /SEQ FA_SDX
COMP <BIN>FA.COM FA_SDX.OBJ 3
FA /SEQ FA_BWD
COMP <DOS>FA.COM FA_BWD.OBJ 3
DEL *.OBJ
CD <TESTS
;===================================
;End Test FA for BW-DOS
;===================================
; last test with invalid source name
; should abort with file name
FA DOES_NOT_EXISTS
